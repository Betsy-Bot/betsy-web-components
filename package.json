{
    "name": "betsy-admin-panel",
    "description": "An Aurelia 2 client application.",
    "version": "2.0.0",
    "repository": {
        "type": "git",
        "url": "???"
    },
    "license": "UNLICENSED",
    "scripts": {
        "lint:js": "eslint src test --ext .js,.ts",
        "lint:html": "htmlhint -c .htmlhintrc src",
        "lint": "npm run lint:js && npm run lint:html",
        "pretest": "npm run lint",
        "start": "vite",
        "build": "rimraf dist && webpack --env production",
        "build:staging": "rimraf dist && webpack --env=NODE_ENV=staging",
        "analyze": "rimraf dist && webpack --env production --analyze",
        "update-services": "npx orval",
        "test": "jest"
    },
    "dependencies": {
        "@aurelia/router-lite": "dev",
        "@aurelia/state": "dev",
        "@aurelia/router": "dev",
        "@aurelia/i18n": "dev",
        "@aurelia/compat-v1": "dev",
        "@aurelia/dialog": "dev",
        "@aurelia/validation-html": "dev",
        "@aurelia/runtime-html": "dev",
        "@darkguy10/discord-markdown": "2.6.0",
        "@emoji-mart/data": "^1.1.2",
        "@material/banner": "^14.0.0",
        "@material/card": "^14.0.0",
        "@material/circular-progress": "^14.0.0",
        "@material/dialog": "^14.0.0",
        "@material/drawer": "^14.0.0",
        "@material/fab": "^14.0.0",
        "@material/icon-button": "^14.0.0",
        "@material/list": "^14.0.0",
        "@material/select": "^14.0.0",
        "@material/switch": "^14.0.0",
        "@material/tab-bar": "^14.0.0",
        "@material/textfield": "^14.0.0",
        "@material/top-app-bar": "^14.0.0",
        "@microsoft/signalr": "^7.0.4",
        "@popperjs/core": "^2.11.7",
        "@rollup/plugin-node-resolve": "^15.0.2",
        "aurelia": "dev",
        "aurelia-fetch-client": "1.8.2",
        "bootstrap": "^5.2.3",
        "devextreme": "^22.2.5",
        "emoji-mart": "^5.5.2",
        "flatpickr": "^4.6.13",
        "ink-mde": "^0.25.0",
        "lets-toast": "^1.0.1",
        "rollup": "^3.20.2"
    },
    "devDependencies": {
        "@aurelia/vite-plugin": "dev",
        "@aurelia/webpack-loader": "latest",
        "@types/jest": "^29.5.0",
        "@types/node": "^18.15.11",
        "autoprefixer": "^10.4.14",
        "css-loader": "^6.7.3",
        "dotenv-webpack": "^8.0.1",
        "html-webpack-plugin": "^5.5.0",
        "copy-webpack-plugin": "^11.0.0",
        "htmlhint": "^1.1.4",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "jest-transform-stub": "^2.0.0",
        "postcss": "^8.4.21",
        "postcss-loader": "^7.2.4",
        "rimraf": "^4.4.1",
        "sass": "^1.60.0",
        "sass-loader": "^13.2.2",
        "style-loader": "^3.3.2",
        "ts-loader": "^9.4.2",
        "tslib": "^2.5.0",
        "typescript": "^5.0.3",
        "vite": "^4.2.1",
        "eslint": "^8.37.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-cypress": "^2.13.2",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "@typescript-eslint/eslint-plugin": "^5.57.1",
        "@typescript-eslint/parser": "^5.57.1",
        "webpack": "^5.75.0",
        "webpack-bundle-analyzer": "^4.7.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "4.11.1"
    },
    "jest": {
        "testMatch": [
            "<rootDir>/test/**/*.spec.ts"
        ],
        "testEnvironment": "jsdom",
        "transform": {
            "\\.(css|less|sass|scss|styl|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "jest-transform-stub",
            "\\.(ts|html)$": "@aurelia/ts-jest"
        },
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.ts",
            "!src/**/*.d.ts"
        ],
        "globals": {
            "ts-jest": {
                "isolatedModules": true
            }
        }
    }
}
